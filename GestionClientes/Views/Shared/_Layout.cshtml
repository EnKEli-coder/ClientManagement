<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GestionClientes</title>
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Icons.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Orders/OrderDetails.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/GestionClientes.styles.css" asp-append-version="true" />
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    
</head>
<body>
    <header id="page-header">
        <nav class="site-nav">
            <button id="btn-clients" onclick="showClientsModule()">Clientes</button>
            <button id="btn-clients" onclick="showOrdersModule()">Pedidos</button>
        </nav>
    </header>
    <div class="container">
        <main class="main">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!--<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>-->
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Clients.js" asp-append-version="true"></script>
    <script src="~/js/Orders.js" asp-append-version="true"></script>
    <script src="~/js/ProductList.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
